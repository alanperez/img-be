    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7"
  }
